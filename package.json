{
  "name": "yahoo-fantasy-basketball-start-active-players",
  "version": "1.0.0",
  "description": "Automatically start active players for your Yahoo Fantasy Basketball team across the entire week. Bypass Yahoo's paywall for the 'Start Active Players' feature with one-click automation.",
  "scripts": {
    "build": "tsc && vite build && node scripts/build.js",
    "watch": "tsc --watch & vite build --watch & node scripts/build.js",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.254",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "sharp": "^0.33.1",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}